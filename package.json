{
  "name": "zentosys-ai-pr-guard",
  "version": "0.1.0",
  "description": "AI-powered PR code review & security guard",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "quality:check": "npm run lint && npm run format:check && npm run typecheck",
    "audit:check": "npm audit --audit-level=high",
    "ai:review": "ts-node ai/ai-review.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "openai": "^6.15.0"
  }
}
