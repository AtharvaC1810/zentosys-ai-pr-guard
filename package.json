{
  "name": "zentosys-ai-pr-guard",
  "version": "0.1.0",
  "description": "AI-powered PR code review & security guard",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "quality:check": "npm run lint && npm run format:check && npm run typecheck",
    "audit:check": "npm audit --audit-level=high"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
